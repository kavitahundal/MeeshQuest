<commands
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="part3in.xsd"
    spatialWidth="16"
    spatialHeight="16"
    pmOrder="3"
    leafOrder="3">

<!-- setup -->
<createCity name="A" x="0" y="0" radius="1" color="red" id="1" z="0"/>
<createCity name="B" x="1" y="0" radius="1" color="red" id="2" z="0"/>
<createCity name="C" x="4" y="0" radius="1" color="red" id="3" z="0"/>

<!-- roadViolatesPMRules --> 
<mapRoad start="A" end="B" id="4"/>

<!-- endPointDoesNotExist -->
<mapRoad start="A" end="P" id="5"/>

<!-- success -->
<mapRoad start="A" end="C" id="5"/>

<!-- portalViolatesPMRules -->
<mapPortal name="P" x="0" y="1" z="0" id="6"/>

<!-- duplicatePortalCoordinates -->
<mapPortal name="P" x="1" y="0" z="0" id="7"/>

<!-- portalIntersectsRoad -->
<mapPortal name="P" x="2" y="0" z="0" id="8"/>

<!-- portalIntersectsRoad (priority over portalViolatesPMRules) -->
<mapPortal name="P" x="3" y="0" z="0" id="9"/>

<!-- duplicatePortalCoordinates -->
<mapPortal name="P" x="0" y="0" z="0" id="10"/>

<!-- success -->
<mapPortal name="P" x="6" y="6" z="0" id="11"/>

<!-- redundantPortal -->
<mapPortal name="P1" x="10" y="10" z="0" id="12"/>

</commands>
